{
  "name": "expo-mfe",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run codegen && expo start --web",
    "start:local": "cp App-Main.ts App.ts && ENV=local npm run codegen && expo start --web",
    "start:prod": "cp App-Main.ts App.ts && ENV=prod npm run codegen && expo start --web",
    "start:explorer": "cp App-Explorer.ts App.ts && expo start --web --config app-explorer.json",
    "build:web": "cp App-Main.ts App.ts && npm run codegen && expo build:web && npm run post-build-web",
    "build:web:aks": "cp App-Main.ts App.ts && npm run test && ENV=aks npm run codegen && expo build:web && npm run post-build-web",
    "build:web:prod": "cp App-Main.ts App.ts && npm run test && ENV=prod npm run codegen && expo build:web && npm run post-build-web",
    "post-build-web-old": "rsync -av ./public/ ./web-build/",
    "format": "prettier --write \"{explorer,src}/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"{explorer,src}/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit -p .",
    "jest": "jest --coverage",
    "post-build-web": "cp -a public/. web-build/",
    "test": "npm run codegen && npm run validate && npm run lint && npm run format:check",
    "test-old": "npm run codegen && npm run validate && npm run lint && npm run format:check && npm run typecheck",
    "docgen": "jsonschema2md -o documentation/src/markdown -d src/schema && cd documentation && gatsby develop",
    "validate-scene": "ts-node ./scripts/validateScenes",
    "validate-navigation": "ajv --errors=text -s src/schema/navigation.schema.json -d app/navigation.json -r src/schema/defs.schema.json",
    "validate-model": "ajv --errors=text -s src/schema/model.schema.json -d \"app/models/*.json\" -r src/schema/defs.schema.json",
    "validate-config": "ajv --errors=text -s src/schema/config.schema.json -d app/config.json -r src/schema/defs.schema.json",
    "validate-theme": "path-exists app/theme.json && ajv --errors=text -s src/schema/theme.schema.json -d app/theme.json -r src/schema/defs.schema.json || echo",
    "validate-style": "ajv --errors=text -s src/schema/styles.schema.json -d \"app/styles/*.json\" -r src/schema/defs.schema.json",
    "validate-data": "ajv --errors=text -s src/schema/data.schema.json -d \"app/data/*.json\" -r src/schema/defs.schema.json",
    "validate": " npm run validate-scene && npm run validate-style && npm run validate-navigation && npm run validate-model && npm run validate-config && npm run validate-theme && npm run validate-data",
    "codegen": "ts-node ./scripts/codegen",
    "set-env": "rm ./app/config.json && cp ./app/environments/config.dev.json ./app/config.json && rm ./app/env.json && cp ./app/environments/env.dev.json ./app/env.json",
    "set-env:aks": "rm ./app/config.json && cp ./app/environments/config.aks.json ./app/config.json && rm ./app/env.json && cp ./app/environments/env.aks.json ./app/env.json",
    "set-env:local": "rm ./app/config.json && cp ./app/environments/config.local.json ./app/config.json && rm ./app/env.json && cp ./app/environments/env.local.json ./app/env.json",
    "set-env:prod": "rm ./app/config.json && cp ./app/environments/config.prod.json ./app/config.json && rm ./app/env.json && cp ./app/environments/env.prod.json ./app/env.json",
    "cli:build": "gulp",
    "cli:watch": "gulp watch",
    "build-deploy": "npm run build:web && git add . && git commit -m 'build' && git push",
    "start:web:rr": "expo start --web"
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.1",
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "ckeditor4-react": "^5.1.0",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "expo": "~49.0.23",
    "expo-document-picker": "^12.0.2",
    "expo-font": "^12.0.8",
    "expo-localization": "^15.0.3",
    "expo-status-bar": "~1.6.0",
    "glob": "^11.0.0",
    "glob-promise": "^6.0.7",
    "html-webpack-plugin": "^5.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jspdf": "^2.5.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.30.1",
    "mustache": "^4.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-html-parser": "^2.0.2",
    "react-native": "0.72.10",
    "react-native-calendars": "^1.1305.0",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "2.17.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
    "react-native-step-indicator": "^1.0.3",
    "react-native-svg": "^15.4.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-web": "^0.19.12",
    "react-quill": "^2.0.0",
    "react-select": "^5.8.0",
    "shelljs": "^0.8.5",
    "uuid": "^10.0.0",
    "whatwg-fetch": "^3.6.20",
    "xlsx": "^0.18.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "^8.0.2",
    "@babel/core": "^7.24.9",
    "@babel/node": "^7.24.8",
    "@babel/plugin-transform-react-jsx": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/fetch-mock": "^7.3.8",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/keyv": "^4.2.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/mustache": "^4.2.5",
    "@types/react": "^18.3.3",
    "@types/react-html-parser": "^2.0.6",
    "@types/react-native": "^0.73.0",
    "@types/react-native-calendars": "^1.1264.7",
    "@types/react-test-renderer": "^18.3.0",
    "@types/shelljs": "^0.8.15",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "ajv-cli": "^5.0.0",
    "del": "^7.1.0",
    "eslint": "^9.7.0",
    "eslint-config-kodefox": "^2.0.0",
    "expo-cli": "^6.3.10",
    "fetch-mock": "^10.0.8",
    "fs-extra": "^11.2.0",
    "gulp": "^5.0.0",
    "gulp-chmod": "^4.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.3",
    "path-exists-cli": "^2.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "^18.3.1",
    "tiff.js": "^1.0.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "resolutions": {
    "minimist": "1.2.6",
    "y18n": "4.0.1",
    "json-schema": "0.4.0"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/test/init.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "ios.ts",
      "ios.tsx",
      "android.ts",
      "android.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react",
          "esModuleInterop": true
        }
      }
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true
}
